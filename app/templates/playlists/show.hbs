<div class="">
  <div class="">
    {{tracks-list tracks=model.tracks playlist=model.playlist
      setIsSyncingPlaylist=(action "setIsSyncingPlaylist")
      labels=model.labels}}
    <img class="uploader-icon" src="/assets/uploadericon.png">
  </div>
  <div class="">
    {{playlist-tracks-list playlist=model.playlist
        playlists=model.playlists
        isSyncingPlaylist=isSyncingPlaylist
        setIsSyncingPlaylist=(action "setIsSyncingPlaylist")
        transitionAfterDelete=(route-action "transitionAfterDelete")
    }}
  </div>
</div>
